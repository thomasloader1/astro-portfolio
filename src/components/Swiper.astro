---
import type { Proyect } from '../types/Proyect';
import ProyectsItem from './ProyectsItem.astro';

interface Props{
  slides: Proyect[]
}

const { slides } = Astro.props
---
<swiper-container class="mySwiper " space-between="30" slides-per-view="1.3" pagination="true"
    pagination-clickable="true" >
 {
    slides.map(proyect =>
    (<swiper-slide>
      <ProyectsItem {...proyect} />
    </swiper-slide>))
}
    
  </swiper-container>